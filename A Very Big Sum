#include <stdio.h>
#include <assert.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>

int main()
{
    long int i , n ;
    scanf("%ld",&n);
    long int a[1000] ,sum = 0;
    for(i=0;i<n;i++)
    scanf("%ld",&a[i]);
    for(i=0;i<n;i++)
    sum = sum + a[i];
    printf("%ld",sum);
    return 0;
}
    
